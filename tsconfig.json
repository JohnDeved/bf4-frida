{
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["ES2023"],
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
  }
}