{
  "name": "frida-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "ts-node index",
    "build": "tsc index.ts && npm run build-frida && pkg . -C Gzip -o undefined.exe",
    "build-frida": "frida-compile agent/index.ts -o _agent.js -c",
    "watch": "frida-compile agent/index.ts -o _agent.js -w",
    "dev": "frida -n bf4.exe -l _agent.js"
  },
  "pkg": {
    "assets": ["_agent.js", "node_modules/frida"],
    "targets": ["node16-win-x64"]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "frida": "^15.0.13",
    "pkg": "^5.8.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/frida-gum": "^17.1.0",
    "@types/node": "^16.4.13",
    "frida-compile": "^10.2.4",
    "ts-node": "^10.2.0"
  }
}
